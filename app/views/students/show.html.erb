<h2> Student details </h2>
<h3><%= @student %></h3>
<table>
<thead>
    <tr>
    <th colspan="3"></th>
    </tr>
</thead>
<tbody>
    <% @student_information.each do |details| %>
    <tr>
        <td><%= details["coursenum"] %></td>
        <td><%= details["coursename"] %></td>
        <td><%= details["grade"] %></td>
    </tr>
    <% end %>
</tbody>
</table>
